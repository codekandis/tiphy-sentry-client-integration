{
	"name": "codekandis/tiphy-sentry-client-integration",
	"description": "This library is an integration of `codekandis/sentry-client` into `codekandis/tiphy`.",
	"keywords": [
		"php",
		"tiphy",
		"sentry",
		"client",
		"integration"
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/tiphy-sentry-client-integration/issues",
		"docs": "https://github.com/codekandis/tiphy-sentry-client-integration/blob/master/README.md",
		"source": "https://github.com/codekandis/tiphy-sentry-client-integration/tree/master"
	},
	"require": {
		"php": "^7.3",
		"codekandis/tiphy": "^0",
		"codekandis/sentry-client": "dev-development"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"sensiolabs/security-checker": "^6",
		"phpunit/phpunit": "^8"
	},
	"repositories": {
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\TiphySentryClientIntegration\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\TiphySentryClientIntegration\\Tests\\": [
				"tests/"
			]
		}
	},
	"scripts": {
		"post-update-cmd": [
			"[ ${COMPOSER_DEV_MODE} -eq 0 ] || vendor/bin/security-checker security:check composer.lock"
		]
	}
}
